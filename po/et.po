msgstr "Selle URL skeemiga kasutatakse ootamatut kasutamise protokolli."

#: src/LYUtils.c:3419
msgid "Too many tempfiles"
msgstr "Liiga palju ajutisi faile"

#: src/LYUtils.c:3719
